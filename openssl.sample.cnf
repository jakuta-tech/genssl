[ca]
default_ca = DefaultCA

[DefaultCA]
database = index.txt

[req]
distinguished_name  = req_dn
default_md = sha512
default_bits = 2048

[req_dn]
countryName_default = FR
countryName_min     = 2
countryName_max     = 2
localityName_default= BiduleVille
organizationName_default = BiduleOrg

[clientauth]
extendedKeyUsage    = clientAuth
